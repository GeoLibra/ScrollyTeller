{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions"]
      },
      "useBuiltIns": true,
    }],
    "react",
  ],
  "plugins": [
    "add-module-exports",
    "transform-object-rest-spread",
    "transform-async-to-generator",
  ],
  "ignore": [
    "node_modules/",
  ],
}